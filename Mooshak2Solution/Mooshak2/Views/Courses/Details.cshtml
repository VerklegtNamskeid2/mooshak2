
@{
    ViewBag.Title = "Details";
}

<h2>Course: @ViewBag.model.Course.Title</h2>

<div>
    <h4><a href="/Assignments/Create">Create assignment</a></h4>

    <hr />
    <ul>
    @foreach (var x in ViewBag.model.Assignments)
    {
        <li><a href="/Assignments/Details/@x.ID">@x.Title</a></li>
    }
    </ul>
</div>
<p>
</p>
